{
  "expo": {
    "name": "Smart Learners",
    "slug": "ai-educator-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/**/*",
      "assets/sounds/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartlearners.mobile",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera to capture solution images.",
        "NSPhotoLibraryUsageDescription": "This app accesses photo library to select images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.smartlearners.mobile",
      "versionCode": 4,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "2180cf55-cf82-4972-abfa-328850bad51c"
      }
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-web-browser"
    ]
  }
}
